{
    "title": "QuickLaunch Finder",
    "rules": [
        {
            "description": "Command+EでFinderを開く",
            "manipulators": [
                {
                    "conditions": [
                        {
                            "bundle_identifiers": [
                                "^com\\.parallels\\.desktop\\.console"
                            ],
                            "type": "frontmost_application_unless"
                        }
                    ],
                    "from": {
                        "key_code": "e",
                        "modifiers": {
                            "mandatory": [
                                "left_command"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open ~"
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}
